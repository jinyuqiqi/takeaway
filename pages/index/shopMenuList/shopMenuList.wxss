.shop .shopTop{
  margin:30rpx 80rpx;
  margin-bottom:40rpx;
}
.shop .shopTop>image{
  width:150rpx;
  height:200rpx;
  float: left;
  margin-right: 80rpx;
}
.shop .shopTop>view image{
  width:36rpx;
  height:35rpx;
}
.shop .shopTop>view view:nth-child(2){
  margin-top:30rpx;
  margin-bottom: 30rpx;
} 
.shop .shopTop>view view:nth-child(3){
  font-size: 28rpx;
  color: #9c9a9c;
} 

.shop .shopBottom{
    font-size: 35rpx;
    border-top:1rpx solid #e7e3e7;
    border-bottom: 20rpx solid #e7e3e7;
    padding-top:30rpx;
    padding-bottom:30rpx;
}
.shop .shopBottom text{
  display: inline-block;
  width:33%;
  text-align: center;
}
.shop .shopBottom text:nth-child(2){
  border-left:1rpx solid #e7e3e7;
  border-right: 1rpx solid #e7e3e7;
}
/*******商家服务*********  */
.service{
    border-bottom: 20rpx solid #e7e3e7;
}
.service .serviceTop{
  width:90%;
  margin:0rpx auto;
  border-bottom: 1rpx solid #e7e3e7;
  padding-top:30rpx;
  padding-bottom: 30rpx;
  font-size: 36rpx;
}
.service .serviceTop image{
  width:46rpx;
  height:45rpx;
  margin-right:30rpx;
}
.service .serviceBottom{
  display: flex;
  padding: 30rpx;
  font-size: 30rpx;
}
.service .serviceBottom view{
  flex:0 0 25%;
  text-align: center
}
.service .serviceBottom view image{
  width:56rpx;
  height:55rpx;
  display: block;
  margin:0 auto;
  margin-bottom:20rpx;
}
/*店铺信息  */
.shopInfo{
  padding-left:40rpx;
  padding-right: 30rpx;
  border-bottom:20rpx solid #e7e3e7;
  font-size: 36rpx;
}
.shopInfo image{
  width:46rpx;
  height:45rpx;
  vertical-align: bottom;
}
.shopInfo view{
  padding-top:30rpx;
  padding-bottom: 30rpx;
  position: relative;
}
.shopInfo view:not(:last-child){
  border-bottom: 1rpx solid #e7e3e7;
}
.shopInfo view .more{
  position: absolute;
  top:35rpx;
  right:10rpx;
}
/*支付方式  */
.payMethod{
  padding:30rpx 40rpx;
  border-bottom:20rpx solid #e7e3e7;
  font-size: 36rpx;
}
.payMethod text{
  padding:5rpx;
  border:1rpx solid #84c3ff;
  border-radius:10rpx;
  color:#84c3ff;
  font-size: 30rpx;
}
/*营业许可证  */
.licence{
  padding:30rpx 20rpx;
  border-bottom:20rpx solid #e7e3e7;
}
.licence view{
  margin: 0 auto;
  border-bottom:1rpx solid #e7e3e7; 
  padding-bottom: 30rpx;
  font-size: 36rpx;
}
/*举报商家  */
.report{
  padding-top:30rpx;
  padding-bottom: 30rpx;
  text-align: center;
  font-size: 36rpx;
  color:#ef1c10;
  border-bottom:20rpx solid #e7e3e7;
}


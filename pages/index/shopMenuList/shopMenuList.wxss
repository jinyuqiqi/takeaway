@import "../../template/tel_swiper/tel_swiper.wxss";

.nav{
  height:80rpx;
  line-height: 80rpx;
  background: #fe2d4a;
  color:#fff;
  display: flex;
  justify-content: space-between;
}
.nav text{
  margin-left:180rpx;
}
.nav wx-image {
  width:46rpx;
  height:45rpx;
  margin-top:18rpx;
  margin-left:15rpx;
}

.nav .star{
  width:40rpx;
  height:40rpx;
  margin-left:100rpx;
}
.nav icon{
  margin-right: 25rpx;
  margin-top:18rpx;
}
/*营业时间  */
.businessTime{
  background:#ffffcb;
  font-size: 28rpx;
  color:#fb9703;
  padding:18rpx 0 18rpx 8.4rpx;
}
.businessTime image{
  width:32rpx;
  height:32rpx;
  margin-top:-5rpx;
  margin-left:14.4rpx;
  vertical-align: middle
}
/******************************************/

.page{
    font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
    height: 100%;
    background: #fff;
    overflow: hidden;
}
.page .nav{
    position: relative;
    z-index: 99;
    display: flex;
    border-top: 1px solid #d1d3d4;
    border-bottom: 1px solid #d1d3d4;
    background: #fff;
}
.page .nav .nav-son{
    display: flex;
    flex: 1;
    text-align: center;
    height: 40px;
    align-items:center;
    justify-content:center;
    font-size: 14px;
    color:#000;
}
.page .nav .content{
    display: inline-block;
}
.page .temp{
    position: relative;
    z-index: 4;
    margin-bottom:1000rpx;
}
.page .temp1,.temp2,.temp3{
    display: none;
    width: 100%;
    height:199rpx;
    overflow-y: scroll;
    padding: 0 0 0 20rpx;
}
.slidedown{
    transform:  translateY(0%);
}

.quyu{
    position: relative;
    height: 100%;
    z-index: 5;
}
.quyu .qy{
    overflow-y: scroll;
    float: left;
    height: 100%;
    line-height: 80rpx;
    box-sizing: border-box; 
    font-size: 14px;
    color: #717273;
}
.qy view{
    text-overflow: ellipsis;
    overflow: hidden; 
    white-space:nowrap;  
    padding-left: 15rpx; 
}
.quyu-left{
  text-align: center;
    background: #f4f4f4;
    width: 30%;
}
.quyu-left view{
  border-bottom:1rpx solid #e4e4e4;
}
.quyu-center{
    width: 70%;
}


.current{
    background: #fff;
}
.current2{
    background: #fff;
}
.nav-son.active{
    border-bottom:5rpx solid #fe2d4a;
}
.nav-son.active .content{
    color: #fe2d4a;
}

/**********************************************我写的  */
.quyu-center>view{
    margin-top:30rpx;
    display: flex;
    align-items: flex-start;
    border-bottom: 1rpx solid #eee;
} 
.page .quyu .quyu-center .foodImg{
  width:150rpx;
  height:100rpx;
  margin-top:30rpx
}
.page .quyu .quyu-center .foodInfo{
  margin-top: -20rpx;
  position: relative
}
.page .quyu .quyu-center .foodInfo text{
  height:30rpx;
}
.page .quyu .quyu-center .foodInfo text:not(last-child){
  margin-bottom: 10rpx;
}
.page .quyu .quyu-center .foodInfo .foodName{
  display: block;
  font-size: 28rpx;
  font-weight: bold;
  color:#000;
}
.page .quyu .quyu-center .foodInfo .foodDetail{
  display: block;
  width:100%;
  height:65rpx;
  margin-bottom:-30rpx;
  overflow: hidden; 
  white-space: nowrap; 
  text-overflow: ellipsis; 
}
.page .quyu .quyu-center .foodInfo .saleNum{
  display: block;
}
.page .quyu .quyu-center .foodInfo .price{
  margin-right:10rpx;
  color:#fa4f45;
  font-size: 40rpx;
  font-weight: bold
}
.page .quyu .quyu-center .foodInfo .originPrice{
  text-decoration: line-through
}
.page .quyu .quyu-center .foodInfo .zhekou{
  display: block;
  margin-top:-40rpx;
  margin-bottom: 50rpx;
  margin-left:20rpx;
  color:#ff958b;
}
.page .quyu .quyu-center .foodInfo .Discount{
  width:24rpx;
  height:24rpx;
  position: absolute;
  top:180rpx;left:10rpx;
}


/***************************************评价  */
.topScore{
  width:100%;
  height:240rpx;
  border-top: 20rpx solid #efefef;
  border-bottom:20rpx solid #efefef;
  display:flex;
  align-items: center
}
.topScore>view{
  flex: 1;
}
.topScore .leftScore{
  flex:0 0 25%;
  text-align: center
}
.topScore .centerScore{
  flex:0 0 50%;
  text-align: center;
  font-size:28rpx;
  color:#636563;
}
.topScore .rightScore{
  flex:0 0 25%;
  text-align: center
}
.topScore .centerScore image{
   width:28.8rpx;height:28.8rpx;
}
.topScore .leftScore{
  font-size: 28rpx;
  color:#636563;
}
.topScore .rightScore{
  font-size: 28rpx;
  color:#cecfce;
  border-left:1rpx solid #dedbde;
}
.topScore .leftScore text{
  display: block;
  font-size: 42rpx;
  color:#ffd363;
  margin-bottom:20rpx;
}
.topScore .rightScore text{
  display: block;
  font-size: 42rpx;
  color:#a5a6a5;
  margin-bottom:20rpx;
}
/*中间  */
.topScore .centerScore image:nth-child(1){
  margin-left:20rpx;
}
.topScore .centerScore text{
  margin-left:20rpx;
}
.topScore .centerScore image{
  vertical-align: middle;
}
.topScore .centerScore .first{
  margin-bottom: 20rpx;
}
/*-----------------大众点评  */
.publicScore {
  width:100%;
  height:80rpx;
  line-height: 80rpx;
  border-bottom:20rpx solid #efefef;

  display: flex;
  justify-content: space-between;
}


.publicScore text:nth-child(1){
  font-size:30rpx;
  color:#6b696b;
  margin-left:20rpx;
}
.publicScore text:nth-child(2){
  font-size:28rpx;
  color:#9c9a9c;
  margin-left:333rpx;
}
.publicScore .more{
  width:46rpx;
  height:46rpx;
  margin-top:15rpx;
}
/*******评价分类-  */
.evaluateSort{
   padding:15rpx 25rpx;
   padding-bottom: 0;
   border-bottom: 1rpx solid #cecfce;
 }
 .evaluateSort text{
   display: inline-block;
   padding: 15rpx 25rpx;
   border:1rpx solid #cecfce;
   font-size:28rpx;
   margin-right:25rpx;
   margin-bottom: 25rpx;
   border-radius: 3rpx;
 }
.on{ color: #da7c0c;  border:1rpx solid #da7c0c !important}
  
.evaluate{ display: block; height: 100%; width: 100%; overflow: hidden; }  
.evaluate view{  
    text-align: center;  
}
.evaluate .evaluateList {
  padding-bottom:30rpx;
  border-bottom: 1rpx solid #dedbde;
}
.evaluate .evaluateList .evlTop .headImg{
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  vertical-align:top;
}
.evaluate .evaluateList .evlTop .star{
  width:30rpx;
  height:30rpx;
}
.evaluate .evaluateList .evlTop{
  padding: 30rpx;
  display:flex;
}
.evaluate .evaluateList .evlTop .evlLeft{
  flex:0 0 60%;
  display: flex;
  align-items: flex-start
}
.evaluate .evaluateList .evlTop .evlTime{
  flex:0 0 30%;
  text-align: right;
  padding-left: 60rpx;
  font-size: 30rpx;
  color:#636563;
}
.evaluate .evaluateList .evlTop .evlLeft .elvLeftR{
  flex: 1;
}
.evaluate .evaluateList .evlTop .evlLeft .uName{
  display: block;
  text-align: left;
  margin-left:20rpx;
  margin-bottom: 20rpx;
}
.evaluate .evaluateList .evlTop .evlLeft .arrive{
  font-size: 30rpx;
  margin-left:20rpx;
  color:#636563;
}
/*****/
.evaluate .evaluateList .evlBottom{
  text-align: left;
  margin-left:130rpx;
}
.evaluate .evaluateList  .evlBottom .evlContent{

}
.evaluate .evaluateList  .evlBottom image{
  width:316rpx;
  height:316rpx;
  margin-top:50rpx;
  margin-bottom: 20rpx;
  display: block;
}
.evaluate .evaluateList  .evlBottom .reply{
  display: block;
  padding: 20rpx;
  margin-right:30rpx;
  background: #efefef;
  font-size: 33rpx;
  color:#636563;
  text-align: left
}

